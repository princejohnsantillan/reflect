{
    "name": "princejohnsantillan/reflect",
    "description": "Clearer API for PHP Reflection",
    "license": "MIT",
    "authors": [
        {
            "name": "Prince John Santillan",
            "email": "mail@princejohnsantillan.net"
        }
    ],
    "keywords": [
        "reflect",
        "reflection",
        "enum",
        "attribute"
    ],
    "autoload": {
        "psr-4": {
            "PrinceJohn\\Reflect\\": "src/"
        }
    },
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.12",
        "laravel/pint": "^1.17"
    },
    "scripts": {
        "lint": [
            "./vendor/bin/pint"
        ],
        "analyse": [
            "./vendor/bin/phpstan analyse"
        ],
        "review": [
            "@lint",
            "@analyse"
        ]
    }
}